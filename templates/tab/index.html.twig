{% extends 'template.html.twig' %}

{% block body %}
<table class="table">
    {% for note in notes %}
    <tr>
        <td>{{ note }}</td>
    </tr>
    {% endfor %}
</table>
<div class="alert alert-info">
    <p>Nombre des notes : {{ notes | length }}</p>
    <p>la note min : {{ min(notes) }}</p>
    <p>la note max : {{ max(notes) }}</p>
</div>
{% endblock %}